    "type": "git",
    "url": "git+https://github.com/vega/vega-lite-dev-config.git"
  },
  "author": "Kanit \"Ham\" Wongsuphasawat, Dominik Moritz",
  "collaborators": [
    "Kanit Wongsuphasawat <kanitw@gmail.com> (http://kanitw.yellowpigz.com)",
    "Dominik Moritz <domoritz@cs.washington.edu> (https://www.domoritz.de)"
  ],
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vega/vega-lite-dev-config/issues"
  },
  "homepage": "https://github.com/vega/vega-lite-dev-config#readme",
  "dependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@beemo/cli": "^1.0.11",
    "@beemo/core": "^1.1.8",
    "@beemo/driver-babel": "^1.2.5",
    "@beemo/driver-eslint": "^1.2.3",
    "@beemo/driver-jest": "^1.2.3",
    "@beemo/driver-prettier": "^1.3.2",
    "@beemo/driver-typescript": "^1.4.0",
    "@types/jest": "^26.0.8",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.2.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "jest": "^26.2.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4"
  },
  "devDependencies": {
    "@types/node": "^14.0.27",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "typescript": "^3"
  },
  "beemo": {
    "module": "@local",
    "drivers": [
      "prettier"
    ]
  },
  "scripts": {
    "prepare": "tsc && beemo create-config",
    "lint": "beemo prettier --check \"./src/**/*.ts\"",
    "format": "beemo prettier --write \"./src/**/*.ts\""
  }
}
